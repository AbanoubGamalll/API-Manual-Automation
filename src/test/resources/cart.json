{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Cart",
  "type": "object",
  "required": [
    "id",
    "userId",
    "date",
    "products",
    "__v"
  ],
  "properties": {
    "id": {
      "type": "integer"
    },
    "userId": {
      "type": "integer"
    },
    "date": {
      "type": "string",
      "format": "date-time"
    },
    "products": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "productId",
          "quantity"
        ],
        "properties": {
          "productId": {
            "type": "integer"
          },
          "quantity": {
            "type": "integer",
            "minimum": 1
          }
        }
      }
    },
    "__v": {
      "type": "integer"
    }
  }
}
